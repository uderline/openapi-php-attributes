{
    "name": "akiosarkiz/openapi-php-attributes",
    "description": "Automatically render your OpenApi 3 file describing your PHP API using attributes",
    "keywords": ["api", "php", "json", "attributes", "openapi", "renderer", "api-rest", "openapi3", "php8"],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Martell",
            "email": "contact@josephmartell.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/finder": "^5.3",
        "symfony/http-foundation": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "OpenApiGenerator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OpenApiGenerator\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.3"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
